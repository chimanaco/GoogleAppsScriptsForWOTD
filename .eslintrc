{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": "airbnb/base",
  "rules": {
    "no-unused-vars": [1, {"vars" : "all", "args" : "after-used"} ],
    "no-new": [0],
    "no-undef": [0],
    "no-shadow": [0],
    "no-unused-expressions": [0],
    "no-new-wrappers": [0],
    "consistent-return": [0],
    "spaced-comment": [0],
    "no-use-before-define": [0],
    "no-param-reassign": [1],
    "global-require": [0]
  },
  "parserOptions": {
    "ecmaVersion": 6,       // ES2015(ES6) の構文はここに集約
    "sourceType": "module", // Modules の指定だけは分離
    "ecmaFeatures": {       // ECMAScript に含まれない仕様はここ
      "jsx": true
    }
  }
}
